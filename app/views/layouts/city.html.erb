
<%= favicon_link_tag() %>
<%= favicon_link_tag('apple-touch-icon-180x180.gif', {:rel => 'apple-touch-icon', :sizes => '180x180', :type => 'image/gif'}) %> 

    <%= csrf_meta_tag %>
    <%= stylesheet_link_tag "temp/header", :media => "all" %>
    <%= stylesheet_link_tag "top", :media => "all" %>
    <%= stylesheet_link_tag "temp/mnavi", :media => "all" %>
    <%= stylesheet_link_tag "temp/footer", :media => "all" %>
    <%= javascript_include_tag "drawer" %>

    
  <head>
      <title><%= page_title %></title>
      <%= display_meta_tags(default_meta_tags) %>
      <%= analytics_init if Rails.env.production? %>
      <!-- Global site tag (gtag.js) - Google Analytics -->
            <script async src="https://www.googletagmanager.com/gtag/js?id=UA-159333918-1"></script>
            <script>
            window.dataLayer = window.dataLayer || [];
            function gtag(){dataLayer.push(arguments);}
            gtag('js', new Date());

            gtag('config', 'UA-159333918-1');
            </script>
  </head>

<body>

    <div id="wrapper">
           <%= render 'shared/header' %>
           <div id="fmessage"><p class="notice"><%= notice %></p><p class="alert"><%= alert %></p></div>
           <%= yield(:main) %>
           <%= yield(:mnavi) %>
           <%= render 'shared/sfeature2' %>
           <%= render 'shared/footer' %>
           <%= render 'shared/bottom_navi' %>
    </div>

</body>

</html>