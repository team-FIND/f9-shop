
<% @page_title = $out.outname1 %> 
<% @title1 = $eshop.shopname %>
<% @title2 = $out.outname1 %> 


<% $outs.each do |out| %>

	<% if $out.outimg1.present? %>
		<% @image1 = image_tag $out.outimg1_url %>
	<% else %>
		<% @image1 = image_tag "template/logo3" %>
	<% end %>

	<% if $out.outimg2.present? %>
		<% @image2 = image_tag $out.outimg2_url %>
	<% else %>
		<% @image2 = image_tag "template/logo3" %>
	<% end %>

	<% if $out.outimg3.present? %>
		<% @image3 = image_tag $out.outimg3_url %>
	<% else %>
		<% @image3 = image_tag "template/logo3" %>
	<% end %>

<% end %>

