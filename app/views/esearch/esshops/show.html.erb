
	<p id="notice"><%= notice %></p>

	<%= content_for(:head)  do %>
	    <meta name="f9-shop.jp" content="FIND　F9　f9 f9-shop.jp">
	    <title><%= page_title %></title>
	<% end %>

	<%= content_for(:main)  do %>
		<div id="main-feature">
			  <%= javascript_include_tag "shop" %>
			  <%= javascript_include_tag "slick.min" %>
  
					<div id="feature-wrapper">
						  <%= stylesheet_link_tag "slick/slick.css", :media => "screen" %>
						  <%= stylesheet_link_tag "slick/slick-theme.css", :media => "screen" %>

						  <ul class="slider">
								<% if $eshop.shopimage1.present? %>
									  <li><%= image_tag $eshop.shopimage1_url %><p><%= $eshop.shopname %></p></li>
								<% else %>
									  <li><%= image_tag "template/homepics" %><p><%= $eshop.shopname %></p></li>
								<% end %>
								<% if $eshop.shopimage2.present? %>
									  <li><%= image_tag $eshop.shopimage2_url %><p>image2</p></li>
								<% else %>
									  <li><%= image_tag "template/homepics" %><p>image2</p></li>
								<% end %>
								<% if $eshop.shopimage3.present? %>
									  <li><%= image_tag $eshop.shopimage3_url %><p>image3</p></li>
								<% else %>
									  <li><%= image_tag "template/homepics" %><p>image3</p></li>
								<% end %>
						  </ul>
					</div>

					<div id="top_af1">
                      <a href="https://px.a8.net/svt/ejp?a8mat=3B7GQK+94NAPE+50+2HN8K1" rel="nofollow">
                      <img border="0" width="728" height="90" alt="" src="https://www23.a8.net/svt/bgt?aid=200222588552&wid=001&eno=01&mid=s00000000018015057000&mc=1"></a>
                      <img border="0" width="1" height="1" src="https://www19.a8.net/0.gif?a8mat=3B7GQK+94NAPE+50+2HN8K1" alt="">
                	</div>
		</div>
<% end %>

		<%= content_for(:idc1)  do %>
			<% $emenucs.each do |emenuc| %>
					<div class="line">
						<%= link_to esearch_esshop_esmenuc_path(esshop_id:$eshop, id:emenuc) do %>
							<li id="mc"><%= emenuc.name %></li><br>
						<%end%>
					</div>
			<% end %>
		<% end %>

		<%= content_for(:idc2)  do %>
