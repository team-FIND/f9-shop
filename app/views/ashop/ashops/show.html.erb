

	<p id="notice"><%= notice %></p>

	<%= content_for(:head)  do %>
	    <title><%= page_title %></title>
      <% end %>
      
      <%= content_for(:main)  do %>
            <div id="main-feature">
                  <%= javascript_include_tag "shop" %>
                  <%= javascript_include_tag "slick.min" %>
      
                        <div id="feature-wrapper">
                              <%= stylesheet_link_tag "slick/slick.css", :media => "screen" %>
                              <%= stylesheet_link_tag "slick/slick-theme.css", :media => "screen" %>

                              <ul class="slider">
                                    <% if $ashop.shopimage1.present? %>
                                          <li><%= image_tag $ashop.shopimage1_url %><p><%= $ashop.shopname %></p></li>
                                    <% else %>
                                          <li><%= image_tag "template/homepics" %><p><%= $ashop.shopname %></p></li>
                                    <% end %>
                                    <% if $ashop.shopimage2.present? %>
                                          <li><%= image_tag $ashop.shopimage2_url %><p>image2</p></li>
                                    <% else %>
                                          <li><%= image_tag "template/homepics" %><p>image2</p></li>
                                    <% end %>
                                    <% if $ashop.shopimage3.present? %>
                                          <li><%= image_tag $ashop.shopimage3_url %><p>image3</p></li>
                                    <% else %>
                                          <li><%= image_tag "template/homepics" %><p>image3</p></li>
                                    <% end %>
                              </ul>
                        </div>
            </div>
    <% end %>

	<%= content_for(:idc1)  do %>
              <% $amenucs.each do |amenuc| %>
                    <div class="line">
